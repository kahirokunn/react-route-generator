{
  "name": "react-router-route-generator",
  "version": "0.0.13",
  "description": "React Router route generator",
  "homepage": "https://github.com/kahirokunn/react-router-route-generator",
  "bugs": "https://github.com/kahirokunn/react-router-route-generator/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/kahirokunn/react-router-route-generator.git"
  },
  "license": "MIT",
  "author": "kahirokunn",
  "main": "src/index.js",
  "typings": "src/index.d.ts",
  "bin": {
    "generate-routes": "src/bin/generate-routes.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf lib gen",
    "eslint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "npm run prettier && npm run eslint --fix",
    "prettier": "prettier '**/*.{js,jsx,ts,tsx,json}' --write",
    "publish": ""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.json": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{yml,yaml}": [
      "prettier --write"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "dependencies": {
    "commander": "^6.2.0",
    "compare-func": "^2.0.0",
    "glob": "^7.1.6"
  },
  "devDependencies": {
    "@types/compare-func": "^1.3.0",
    "@types/glob": "^7.1.3",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "eslint-plugin-unicorn": "^23.0.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.2",
    "prettier": "^2.1.2",
    "sort-package-json": "^1.46.0",
    "typescript": "^4.0.3"
  }
}
